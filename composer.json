{
  "name": "mvestil/encryptate",
  "description": "Extended version of Illuminate Encryption package to support rotating encryption keys.",
  "keywords": [
    "encrypt",
    "encryptate",
    "l5",
    "laravel",
    "laravel5",
    "encryption",
    "rotating encryption",
    "rotate",
    "laravel5 encryption"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Bonnie Vestil",
      "email": "mbvestil@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/console": "^5.0",
    "illuminate/encryption": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Encryptate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Encryptate\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.2"
    },
    "laravel": {
      "providers": [
        "Encryptate\\EncryptateServiceProvider"
      ]
    }
  }
}
